{
  "name": "theop-io-static",
  "version": "1.0.0",
  "description": "Static assets for https://theop.io",
  "scripts": {
    "clean": "rimraf dist",
    "codegen": "ts-node ./tools/the-words-build-tool.ts ./src/the-words/the-words-list.txt ./src/the-words/generated/the-words-db.ts",
    "build": "npm run codegen && webpack --config webpack.prod.ts",
    "serve": "npm run codegen && webpack serve --config webpack.dev.ts",
    "lint": "eslint .",
    "format:fix": "prettier --write ./*",
    "build-dev": "npm run codegen && webpack --config webpack.dev.ts"
  },
  "author": "Robin Giese",
  "private": true,
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/youtube-player": "^5.5.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.45.0",
    "html-bundler-webpack-plugin": "^2.0.1",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.20",
    "@floating-ui/dom": "^1.4.4",
    "@types/youtube": "^0.0.47",
    "youtube-player": "^5.6.0"
  }
}
